    </declare-styleable>

    <declare-styleable name="MySlideMenuLayout">
        <attr name="menuWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="VoiceImage">
        <attr name="circleColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="progress" format="integer"/>
        <attr name="vIcon" format="reference"/>
        <attr name="ringWidth" format="dimension"/>
        <attr name="count" format="integer"/>
