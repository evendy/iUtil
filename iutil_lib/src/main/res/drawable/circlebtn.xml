<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:color="@color/menu_circle" android:width="1dp" />
            <solid android:color="@color/menu_bg_norm" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:color="@color/menu_circle" android:width="1dp"/>
            <solid android:color="@color/menu_bg_press" />
        </shape>
    </item>
</selector>